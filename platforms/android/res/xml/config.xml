<?xml version='1.0' encoding='utf-8'?>
<widget id="sajibe.kanti.cmt" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Class Routine</name>
    <description>
        A sample Apps For Understand Class Date and Time 
    </description>
    <author email="Sajibekanti247@gmail.com" href="http://sajibekanti.pw">
       Sajibe kanti 
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/logo.png" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
